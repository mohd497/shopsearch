<% products.each do |product| %>

  <div class="col-xs-4 col-sm-4">
    <div class="box-part">

      <div class="title text-center">
        <h4><%= product.title %></h4>
      </div>

      <div class="text text-center">
        <span><%= product.website %></span>
      </div>

    </div>
  </div>

<% end %>
