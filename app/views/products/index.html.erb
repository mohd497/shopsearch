  <div class="box">
    <div class="container">
      <div>
        <%= render "filter" %>
      </div>
      <div class="row" id="products">
        <%= render "product", products: @products %>
      </div>
      <div class="shop_pagination">
        <div class="page_info">
          <%= page_entries_info @products %>
        </div>
        <%= will_paginate @products, :container => false %>
      </div>
    </div>

    <div id="productModel" class="modal hide fade" role="dialog" aria-labelledby="productModel" aria-hidden="true">
      <div class="modal-dialog" role="document">
        <div class="modal-content"></div>
      </div>
    </div>
  </div>

