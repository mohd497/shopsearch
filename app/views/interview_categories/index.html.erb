<p id="notice"><%= notice %></p>

<h1>Interview Categories</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @interview_categories.each do |interview_category| %>
      <tr>
        <td><%= interview_category.title %></td>
        <td><%= interview_category.description %></td>
        <td><%= link_to 'Show', interview_category %></td>
        <td><%= link_to 'Edit', edit_interview_category_path(interview_category) %></td>
        <td><%= link_to 'Destroy', interview_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Interview Category', new_interview_category_path %>
